{
  "name": "hulo",
  "version": "1.0.0",
  "description": "An app connecting Eckists across the world.",
  "main": "src/main/main.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run clean && npm run compile",
    "test": "jest",
    "start-dev": "nodemon --exec ts-node src/main/main.ts",
    "start": "ts-node src/main/main.ts",
    "style": "eslint '*/**/*.{ts,js}' --fix && prettier --write '**/*.{ts,js,json}'",
    "check-style": "eslint '*/**/*.{ts,js}' && prettier --check '**/*.{ts,js,json}'"
  },
  "author": "Hulo",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.3.1",
    "pino": "^8.14.2",
    "pino-pretty": "^10.2.0"
  }
}
